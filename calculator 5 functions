# Calculator in Python

def add(a, b):
  answer = a + b
  return answer

def subtract(a, b):
  answer = a - b
  return answer
   

def multiply(a, b):
  answer = a * b
  return answer

def divide(a, b):
  answer = a / b
  return answer

def  exp(a, b):
  answer = a ** b
  return answer 

print(add(100, 6))
print(exp(100, 6))
print(subtract(-100, 6))
print(divide(100, 6))
print(multiply(100, -6))
